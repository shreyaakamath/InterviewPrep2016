package cracking.the.coding.interview;

class Node{
	int data;
	Node next;
	
	Node(int n){
		data=n;
	}
}
public class LinkedList {
	Node head;
	
	void append (int val){
		 Node n = new Node(val);
		 if(head==null)head=n;
		 else{
			 head.next=n;
		 }
			 
	}
	
	public static void main(String[] args) {

	}

}
