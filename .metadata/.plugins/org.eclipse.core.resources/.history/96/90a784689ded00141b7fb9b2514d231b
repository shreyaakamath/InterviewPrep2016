package spoj.most.solved.probs;

public class SmallFactorial {

	static void getFactorial(int n){
		double factorial =n;
		for(int i =2;i<n;i++)
			factorial*=i;
		System.out.println("factorial of "+ n +"---"+factorial);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
