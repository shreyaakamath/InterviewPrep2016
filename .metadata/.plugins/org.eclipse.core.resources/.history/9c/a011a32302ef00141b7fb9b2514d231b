package spoj.most.solved.probs;

import java.math.BigInteger;
import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class Candy3 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int test = scan.nextInt();
		List<String> ans = new ArrayList<String>();
		for(int i=0;i<test;i++){
			scan.nextLine();
			BigInteger num = scan.nextBigInteger();
			BigInteger[] val = new BigInteger[num];
			BigInteger total = new BigInteger(0+"");
			for(BigInteger j = BigInteger.ZERO;j<num;j++){ val[j] = scan.nextBigInteger();total= total.add(val[j]);}
			if(total.mod(num) == BigInteger.ZERO) ans.add("YES");
			else ans.add("NO");
		}
		for(String str : ans)
			System.out.println(str);
		
	}

}
